{
    "name": "fidelize/flowchart-to-png",
    "description": "Convert from flowchart json encoded to png",
    "keywords": ["php", "flowchart", "png", "Fidelize"],
    "type": "project",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Fidelize\\Flowchart\\ToPng\\": "src/",
            "Fidelize\\Test\\": "tests/"
        }
    },
    "suggest": {
        "php": ">=7.0"
    },
    "scripts" : {
        "test" : "phpunit",
        "coverage" : "phpunit --coverage-clover=coverage.clover",
        "codacy" : "codacycoverage clover coverage.clover"
    }
}
